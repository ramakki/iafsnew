    <script>
        var cc = 1;
        var startHieght = 0;
        $('#more1').click(function () {
           // alert("JSK");
            var reducedHeight = $('.about_profile').height();

            $('.about_profile').css('height', 'auto');
            var fullHeight = $('.about_profile').height();

            $('.about_profile').height(reducedHeight);
            if (cc == 1) {
                $('.about_profile').animate({ height: fullHeight }, 500);
                startHieght = reducedHeight;
                $('#more1').text('Read Less...');
                cc = 2;

                //if (cc == 2) {
                //    setInterval(function () {
                //        closeMore();
                //    }, 7000);
                //}
                return;
            }
            if (cc==2) {
                $('.about_profile').animate({ height: startHieght }, 500);
                $('#more1').text('Read More...');
                cc = 1;
                return;
            }
        });

    </script>
